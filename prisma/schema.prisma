datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int      @id @default(autoincrement())
  username   String   @unique
  password   String
  firstName String
  lastName  String
  gender     Gender
  birthdate  DateTime
  isAdmin   Boolean  @default(false)
}

enum Gender {
  male
  female
  other
}